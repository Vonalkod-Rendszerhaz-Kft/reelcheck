        <Part Name="CT">^container_type=(?'CT'.*)</Part>
        <Part Name="DQTY">^default_quantity=(?'DQTY'.*)</Part>
        -->
        <Part Name="IPNDESC">^description=(?'IPNDESC'.*)</Part>
