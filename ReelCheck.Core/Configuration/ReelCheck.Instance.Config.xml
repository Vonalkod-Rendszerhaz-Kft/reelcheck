    </HandheldLabel>
    <HandheldLabel Source="CHECK">
      <Label Id="HandheldLabel_CHECK"/>
    </HandheldLabel>
  </HandheldLabels>
  <CheckSuccess NameSeparator="THIS IS A NOT USED ATTRIBUTE">
    <Condition Type="Equal" Test="{FVSCODE}{MTSCODE}" With="{FVSCODE}{MTSCODE}"/>
