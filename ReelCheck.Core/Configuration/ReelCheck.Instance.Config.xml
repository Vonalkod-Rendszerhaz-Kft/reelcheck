    <BookingRecord Name="RESULT_FAIL_DATEAERR" Enabled="true"/>
    <BookingRecord Name="RESULT_FAIL_NOACK" Enabled="true"/>
    <BookingRecord Name="RESULT_FAIL_REFUSE" Enabled="true"/>
