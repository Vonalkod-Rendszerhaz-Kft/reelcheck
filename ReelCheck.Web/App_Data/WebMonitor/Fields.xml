				<Condition Type="Contains" With="WS.CHECKSTATUS" Value="OPERATING" Override="@CHECKStatus_OPERATING@" CellStyle="CellStyle_GreyInverse"/>
				<Condition Type="Contains" With="WS.CHECKSTATUS" Value="READY" Override="@CHECKStatus_READY@" CellStyle="CellStyle_GreenInverse"/>
