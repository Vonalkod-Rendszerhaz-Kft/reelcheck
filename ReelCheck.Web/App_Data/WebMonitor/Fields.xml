				<Condition Type="Contains" With="WS.PRINTSTATUS" Value="WAITING" Override="@PRINTStatus_WAITING@" CellStyle="CellStyle_GreyInverse"/>
				<Condition Type="Contains" With="WS.PRINTSTATUS" Value="READY" Override="@PRINTStatus_READY@" CellStyle="CellStyle_GreenInverse"/>
				<Condition Type="Else" CellStyle="CellStyle_Default"/>
