				<Condition Type="Contains" With="WS.REELCHECKMODE" Value="WAITLOGIN" FormatString="@REELCHECKMODE_WAITLOGIN@ ({0})" CellStyle="CellStyle_GreenInverse"/>
				<Condition Type="Contains" With="WS.REELCHECKMODE" Value="NORMAL" FormatString="@REELCHECKMODE_NORMAL@ ({0})" CellStyle="CellStyle_GreenInverse"/>
				<Condition Type="Contains" With="WS.REELCHECKMODE" Value="GOLDENSAMPLE" FormatString="@REELCHECKMODE_GOLDENSAMPLE@ ({0})" CellStyle="CellStyle_GreenInverse"/>
				<Condition Type="Contains" With="WS.REELCHECKMODE" Value="HANDHELD" FormatString="@REELCHECKMODE_HANDHELD@ ({0})" CellStyle="CellStyle_GreenInverse"/>
				<Condition Type="Contains" With="WS.REELCHECKMODE" Value="RESET" FormatString="@REELCHECKMODE_RESET@ ({0})" CellStyle="CellStyle_RedInverse"/>
				<Condition Type="Else" CellStyle="CellStyle_Default"/>
