				<Condition Type="Contains" With="WS.IDSTATUS" Value="READY" Override="@IDStatus_READY@" CellStyle="CellStyle_GreenInverse"/>
				<Condition Type="Else" CellStyle="CellStyle_Default"/>
			</Conditions>
		</Field>
		<Field Name="F_WS.PRINTSTATUS" Content="WS.PRINTSTATUS" FormatString="{0}" CellStyle="CellStyle_Default">
