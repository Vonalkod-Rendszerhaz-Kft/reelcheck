@model List<string>

<div class="card" style="margin-top: .5rem">
    <div class="card-header">
        <h3>@Html.Raw(String.IsNullOrWhiteSpace(ViewBag.Head) ? (String.IsNullOrWhiteSpace(ViewBag.Title) ? "Error" : ViewBag.Title) : ViewBag.Head)</h3>
    </div>
    <div class="card-body">
        @if (Model != null && Model.Count() > 0)
        {
            foreach (var mess in Model)
            {
                <div>@Html.Raw(mess)</div>
            }
        }
    </div>
</div>
